<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cc.familyaccountbook">

    <application android:icon="@drawable/ffm"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:configChanges="orientation|keyboardHidden"
        android:launchMode="singleTask"
        android:screenOrientation="portrait"
        ><!--禁止转动屏幕，保持竖屏-->

        <activity android:name=".SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GuideActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".Login"
            android:screenOrientation="portrait"
            android:label="@string/login">
        </activity>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name">
        </activity>
        <activity  android:name=".AddOutaccount"
            android:screenOrientation="portrait"
            android:label="@string/addoutaccount">
        </activity>
        <activity  android:name=".AddInaccount"
            android:screenOrientation="portrait"
            android:label="@string/addinaccount">
        </activity>
        <activity  android:name=".Outaccountinfo"
            android:screenOrientation="portrait"
            android:label="@string/outaccountinfo">
        </activity>
        <activity  android:name=".Inaccountinfo"
            android:screenOrientation="portrait"
            android:label="@string/inaccountinfo">
        </activity>
        <activity  android:name=".Showinfo"
            android:screenOrientation="portrait"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask"
            android:label="@string/showinfo">
        </activity>
        <activity  android:name=".Sysset"
            android:screenOrientation="portrait"
            android:label="@string/sysset">
        </activity>
        <activity  android:name=".Accountflag"
            android:screenOrientation="portrait"
            android:label="@string/accountflag">
        </activity>
        <activity  android:name=".InfoManage"
            android:screenOrientation="portrait"
            android:label="@string/infomanage">
        </activity>
        <activity  android:name=".FlagManage"
            android:screenOrientation="portrait"
            android:label="@string/flagmanage">
        </activity>
    </application>

</manifest>